	<script>
        $(function(){
            $(window).load(function(){
                if ($('.crtvword_prmdone').length > 0) {
                   $("html, body").animate({ scrollTop: $(this).height() - 450});
                };
            });
						
            $(window).scroll(function() {				
				if ($(this).scrollTop() > 100) {
					$('.backtotop').fadeIn();
				} else {
					$('.backtotop').fadeOut();
				}
            });
			
			
				//Hours
				$('.btn-hours').click(function(e){
					e.preventDefault();
					$('.hours-list').fadeToggle("fast");
					$(this).toggleClass("btn-hours-active");
				});
				
				$('.hours-close').click(function(e){
					e.preventDefault();
					$('.hours-list').fadeToggle("fast");
					$('.btn-hours').toggleClass("btn-hours-active");
				});
			

			
				  
			//Click event to scroll to top
			$('.backtotop').click(function(){
				$('html, body').animate({scrollTop : 0},800);
				return false;
			});
        });
    </script>
